{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Egypt Chatbot",
      "type": "debugpy",
      "request": "launch",
      "module": "flask",
      "env": {
        "FLASK_APP": "src/app.py",
        "FLASK_ENV": "development"
      },
      "args": ["run", "--no-debugger", "--host=0.0.0.0", "--port=8000"],
      "jinja": true,
      "envFile": "${workspaceFolder}/.env"
    }
  ]
}
